#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/laijiashang/Desktop/carShopping/node_modules/.pnpm/vite@6.3.2_@types+node@20.17.30_jiti@2.4.2_lightningcss@1.29.2_sass@1.87.0_terser@5.39.0_yaml@2.7.1/node_modules/vite/bin/node_modules:/Users/laijiashang/Desktop/carShopping/node_modules/.pnpm/vite@6.3.2_@types+node@20.17.30_jiti@2.4.2_lightningcss@1.29.2_sass@1.87.0_terser@5.39.0_yaml@2.7.1/node_modules/vite/node_modules:/Users/laijiashang/Desktop/carShopping/node_modules/.pnpm/vite@6.3.2_@types+node@20.17.30_jiti@2.4.2_lightningcss@1.29.2_sass@1.87.0_terser@5.39.0_yaml@2.7.1/node_modules:/Users/laijiashang/Desktop/carShopping/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/laijiashang/Desktop/carShopping/node_modules/.pnpm/vite@6.3.2_@types+node@20.17.30_jiti@2.4.2_lightningcss@1.29.2_sass@1.87.0_terser@5.39.0_yaml@2.7.1/node_modules/vite/bin/node_modules:/Users/laijiashang/Desktop/carShopping/node_modules/.pnpm/vite@6.3.2_@types+node@20.17.30_jiti@2.4.2_lightningcss@1.29.2_sass@1.87.0_terser@5.39.0_yaml@2.7.1/node_modules/vite/node_modules:/Users/laijiashang/Desktop/carShopping/node_modules/.pnpm/vite@6.3.2_@types+node@20.17.30_jiti@2.4.2_lightningcss@1.29.2_sass@1.87.0_terser@5.39.0_yaml@2.7.1/node_modules:/Users/laijiashang/Desktop/carShopping/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite@6.3.2_@types+node@20.17.30_jiti@2.4.2_lightningcss@1.29.2_sass@1.87.0_terser@5.39.0_yaml@2.7.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../vite@6.3.2_@types+node@20.17.30_jiti@2.4.2_lightningcss@1.29.2_sass@1.87.0_terser@5.39.0_yaml@2.7.1/node_modules/vite/bin/vite.js" "$@"
fi
