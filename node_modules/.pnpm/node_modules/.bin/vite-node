#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/carShopping/node_modules/.pnpm/vite-node@0.31.4_@types+node@20.17.30_lightningcss@1.29.2_sass@1.87.0_terser@5.39.0/node_modules/vite-node/node_modules:/workspaces/carShopping/node_modules/.pnpm/vite-node@0.31.4_@types+node@20.17.30_lightningcss@1.29.2_sass@1.87.0_terser@5.39.0/node_modules:/workspaces/carShopping/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/carShopping/node_modules/.pnpm/vite-node@0.31.4_@types+node@20.17.30_lightningcss@1.29.2_sass@1.87.0_terser@5.39.0/node_modules/vite-node/node_modules:/workspaces/carShopping/node_modules/.pnpm/vite-node@0.31.4_@types+node@20.17.30_lightningcss@1.29.2_sass@1.87.0_terser@5.39.0/node_modules:/workspaces/carShopping/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../vite-node/vite-node.mjs" "$@"
fi
